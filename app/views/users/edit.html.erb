<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>
<%= form_for @user do |f| %>
	<div class="field">
		<%= f.label :username %>
		<%= f.text_field :username %>
	</div>
	<div class="field">
		<%= f.label :biography %>
		<%= f.text_area :biography %>
	</div>
	<div class="field">
		<%= f.label :phone %>
		<%= f.text_field :phone %>
	</div>
	<div class="field">
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>
	<div class="action">
		<%= f.submit %>
	</div>
<% end %>